{
	"name": "Desk Mess Mirrored",
	"version": "3.5.0",
	"author": [
		"[http://edwardcaissie.com/ Edward Caissie]",
		"Jack Phoenix",
		"Samantha Nguyen"
	],
	"url": "https://www.mediawiki.org/wiki/Skin:DeskMessMirrored",
	"descriptionmsg": "deskmessmirrored-skin-desc",
	"license-name": "GPL-2.0-only",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.40.0"
	},
	"ValidSkinNames": {
		"deskmessmirrored": {
			"displayname": "DeskMessMirrored",
			"class": "SkinTemplate",
			"args": [ {
				"name": "deskmessmirrored",
				"template": "DeskMessMirroredTemplate",
				"bodyOnly": true,
				"bodyClasses": [
					"home",
					"blog",
					"custom-background"
				],
				"styles": [
					"skins.deskmessmirrored"
				]
			} ]
		}
	},
	"MessagesDirs": {
		"SkinDeskMessMirrored": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"DeskMessMirroredTemplate": "includes/DeskMessMirroredTemplate.php",
		"DeskMessMirroredSkinNavigationService": "includes/DeskMessMirroredSkinNavigationService.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "DeskMessMirrored"
	},
	"ResourceModules": {
		"skins.deskmessmirrored": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"features": {
				"content-links": true,
				"content-links-external": true,
				"content-media": true,
				"interface-message-box": true,
				"interface-category": true,
				"content-tables": true,
				"i18n-ordered-lists": true,
				"i18n-all-lists-margins": true,
				"i18n-headings": true,
				"elements": true,
				"interface": true
			},
			"styles": {
				"css/style.css": {
					"media": "screen"
				},
				"css/wordpress-rules.css": {
					"media": "screen"
				},
				"css/reset.css": {
					"media": "screen"
				},
				"css/print.css": {
					"media": "print"
				}
			}
		}
	},
	"manifest_version": 1
}
